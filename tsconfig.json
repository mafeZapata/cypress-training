{
    "compilerOptions": {
        "target": "es5",
        "skipLibCheck": true,
        "strict": true,
        "lib": ["es5", "dom"],
        "types": [
            "cypress",
            "node",
            "cypress-file-upload",
            "cypress-iframe"
        ]
    },
    "include": [
        "cypress/**/*.ts",
        "cypress.config.ts"
    ],
    "exclude": [
        "node_modules/"
    ],
}
